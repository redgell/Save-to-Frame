@import 'library/common.js'

var document;
var onRun = function(context) {
	var options = ['Nexus 5X'],
	selection = context.selection,
		document = context.document
		deviceStyle = createSelectDialog("Choose device type:", options)
	;

    com.redgell.offset_y = -12;
    com.redgell.offset_x = -4;
	com.redgell.scale = 1.0;
	com.redgell.context = context;
	com.redgell.export('nexus5x', context, deviceStyle[1])
}
